{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$ref": "#/definitions/SearchResults",
  "definitions": {
    "SearchResults": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "page": {
          "type": "integer"
        },
        "total_results": {
          "type": "integer"
        },
        "total_pages": {
          "type": "integer"
        },
        "results": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Result"
          }
        }
      },
      "required": [
        "page",
        "results",
        "total_pages",
        "total_results"
      ],
      "title": "SearchResults"
    },
    "Result": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "popularity": {
          "type": "number"
        },
        "vote_count": {
          "type": "integer"
        },
        "video": {
          "type": "boolean"
        },
        "poster_path": {
          "type": "string"
        },
        "id": {
          "type": "integer"
        },
        "adult": {
          "type": "boolean"
        },
        "backdrop_path": {
          "anyOf": [
            {
              "type": "null"
            },
            {
              "type": "string"
            }
          ]
        },
        "original_language": {
          "type": "string"
        },
        "original_title": {
          "type": "string"
        },
        "genre_ids": {
          "type": "array",
          "items": {
            "type": "integer"
          }
        },
        "title": {
          "type": "string"
        },
        "vote_average": {
          "type": "number"
        },
        "overview": {
          "type": "string"
        },
        "release_date": {
          "type": "string",
          "format": "date"
        }
      },
      "required": [
        "adult",
        "backdrop_path",
        "genre_ids",
        "id",
        "original_language",
        "original_title",
        "overview",
        "popularity",
        "poster_path",
        "release_date",
        "title",
        "video",
        "vote_average",
        "vote_count"
      ],
      "title": "Result"
    }
  }
}
